module Main
  include Fmt

  # type (int, int) -> int
  def plus(a, b)
    a + b
  end

  # type (int, int, int) -> int
  def plus_plus(a, b, c)
    a + b + c
  end

  def main
    res = plus(1, 2)

    println("1 + 2 = ", res)

    res = plus_plus(1, 2, 3)

    println("1 + 2 + 3 = ", res)
  end
end
